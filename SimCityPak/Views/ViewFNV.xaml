<Window x:Class="SimCityPak.Views.ViewFNV"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FNV Calculator" Height="152" Width="377" KeyUp="Window_KeyUp_1" ResizeMode="NoResize" WindowStyle="SingleBorderWindow">
    <Grid Height="113">
        <Label Content="Text:" Height="28" HorizontalAlignment="Left" Margin="15,13,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Hash:" Height="28" HorizontalAlignment="Left" Margin="11,47,0,0" Name="label2" VerticalAlignment="Top" />
        <TextBox Height="23" Margin="58,15,12,0" Name="textBox1" VerticalAlignment="Top" TextChanged="textBox1_TextChanged" />
        <TextBox Height="23" Margin="58,49,0,0" Name="textBox2" VerticalAlignment="Top" HorizontalAlignment="Left" Width="165" IsReadOnly="True" />
        <Label Content="Lower Case (Default)" Height="28" HorizontalAlignment="Left" Margin="228,47,0,0" Name="label3" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="58,78,0,0" Name="textBox3" VerticalAlignment="Top" Width="165" IsReadOnly="True" />
        <Label Content="Hash:" Height="28" HorizontalAlignment="Left" Margin="11,76,0,0" Name="label4" VerticalAlignment="Top" />
        <Label Content="Case Sensitive" Height="28" HorizontalAlignment="Left" Margin="228,76,0,0" Name="label5" VerticalAlignment="Top" />
    </Grid>
</Window>
